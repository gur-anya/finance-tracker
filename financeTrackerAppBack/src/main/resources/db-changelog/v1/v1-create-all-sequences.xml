<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="create-user-seq" author="Gureva Anna">
        <createSequence sequenceName="user_id_seq" schemaName="main"/>
    </changeSet>

    <changeSet id="create-trans-seq" author="Gureva Anna">
        <createSequence sequenceName="transactions_id_seq" schemaName="main"/>
    </changeSet>

    <changeSet id="create-login-audit-seq" author="Gureva Anna">
        <createSequence sequenceName="login_audition_id_seq" schemaName="service_schema"/>
    </changeSet>

    <changeSet id="create-user_actions_seq" author="Gureva Anna">
        <createSequence sequenceName="user_actions_id_seq" schemaName="service_schema"/>
    </changeSet>
</databaseChangeLog>